	"devDependencies": {
		"eslint": "8.57.1",
		"grunt-apidoc": "0.11.0",
		"gulp": "4.0.2",
		"gulp-apidoc": "0.2.8",
		"husky": "8.0.3",
		"lint-staged": "12.5.0",
		"nodemon": "2.0.22",
		"prettier": "2.8.8"
	},
	"lint-staged": {
		"../*": "prettier --ignore-unknown --write",
		"*": "prettier --ignore-unknown --write"
	},
