{
	"name": "Entity-Management",
	"version": "1.0.0",
	"description": "Entity service",
	"main": "app.js",
	"bin": {
		"entity-service-migrations": "module/migrations/bin/migrations.js"
	},
	"scripts": {
		"test": "",
		"start": "NODE_ENV=development nodemon app.js",
		"prod": "NODE_ENV=production node app.js",
		"stage": "NODE_ENV=stage node app.js",
		"qa": "NODE_ENV=qa node app.js",
		"prepare": "cd .. && husky install src/.husky",
		"integration": "node app.js",
		"test:integration": "jest --verbose ./integration-test  --config=integrationJest.config.js --runInBand",
		"dev": "nodemon app.js",
		"coverage": " nyc --reporter=lcov mocha --timeout 10000 test --exit"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ELEVATE-Project/entity-management.git"
	},
	"keywords": [
		"shikshalokam"
	],
	"author": "Priyanka Pradeep <priyankapradeepng@gmail.com>",
	"license": "ISC",
	"dependencies": {
		"async": "3.2.6",
		"body-parser": "1.20.3",
		"bunyan": "1.8.15",
		"bunyan-format": "0.2.1",
		"cache-manager": "3.6.3",
		"chai": "4.5.0",
		"chai-http": "4.4.0",
		"cli-table": "0.3.11",
		"cors": "2.8.5",
		"csv-writer": "1.6.0",
		"csvtojson": "2.0.10",
		"dotenv": "8.6.0",
		"elevate-logger": "3.1.0",
		"elevate-services-health-check": "0.0.6",
		"eslint": "8.57.1",
		"express": "4.21.2",
		"express-fileupload": "1.5.2",
		"express-validator": "5.3.1",
		"fn-args": "4.0.0",
		"form-urlencoded": "4.5.1",
		"fs": "0.0.1-security",
		"fs-extra": "9.0.1",
		"json2csv": "5.0.7",
